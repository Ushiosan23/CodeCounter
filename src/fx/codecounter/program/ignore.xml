<?xml version="1.0" encoding="UTF-8" ?>
<root>
	<!-- ignore by name -->
	<item type="name" value=".vs"/>
	<item type="name" value=".idea"/>
	<item type="name" value=".git"/>
	<item type="name" value=".vscode"/>
	<item type="name" value="nbproject"/>
	
	<item type="name" value=".gitignore"/>
	<item type="name" value=".gitmodules"/>
	<item type="name" value=".htaccess"/>
	<item type="name" value="package.json"/>
	<item type="name" value="composer.json"/>
	
	<!-- ignore by regex -->
	<item type="regex" value="^\\.\\w+"/>
	<item type="regex" value="^.*\\.(md|txt|exe|bin|obj|class|lock|a|o)"/>
</root>